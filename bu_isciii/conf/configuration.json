{
    "sftp_copy": {
        "protocol": "rsync",
        "options": ["-rlpv", "--update", "-L", "--inplace"],
        "exclusions": [
            "'*_NC'",
            "'lablog'",
            "'work'",
            "'00-reads'",
            "'*.sh'",
            "'.nextflow*'",
            "'*_DEL'",
            "'*.R'",
            "'*.py'"
        ]
    },
    "api_settings": {
        "server": "http://iskylimsapi.isciii.es",
        "api_url": "/drylab/api/"
    },
    "local_api_settings": {
        "server": "http://iskylims.isciii.es",
        "api_url": "/drylab/api/"
    },
    "bioinfo_doc": {
        "bioinfodoc_path": "/data/bioinfo_doc/",
        "services_path": "services",
        "service_folder": ["request", "resolution", "result"],
        "md_template_path_file" : "templates/jinja_template.j2",
        "html_template_path_file" : "templates/html_service_template.html",
        "path_to_css" : "assets/css"
    },
    "new_service": {
        "fastq_repo": "/srv/fastq_repo"
    },
    "scratch_copy": {
        "command": "rsync -rlv --exclude 'service_info.txt' --exclude 'work' "
    },
    "archive": {
        "data_path": "/data/bi",
        "archived_path": "/archived/bi",
        "protocol": "rsync",
        "options": ["-rv"]
    }
}
